<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aethli.gttss.dao.StudentMapper">
    <resultMap id="BaseResultMap" type="cn.aethli.gttss.domain.Student">
        <id column="userId" property="userId"/>
        <result column="name" property="name"/>
        <result column="classCode" property="classCode"/>
        <result column="gender" property="gender"/>
        <result column="batchCode" property="batchCode"/>
        <result column="gradeName" property="classCode"/>
    </resultMap>
    <sql id="Base_From">
        Student
    </sql>
    <insert id="insertWithAll" parameterType="cn.aethli.gttss.domain.SysUser">
      INSERT INTO <include refid="Base_From" /> (`userId`, `name`, `classCode`, `gender`, `batchCode`, `gradeName`)
        VALUES (#{userId}, #{name}, #{classCode}, #{gender}, #{batchCode},#{genderNam});
    </insert>
</mapper>