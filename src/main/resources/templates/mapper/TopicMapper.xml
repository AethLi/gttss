<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aethli.gttss.dao.TopicMapper">
    <resultMap id="BaseResultMap" type="cn.aethli.gttss.domain.Topic">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="teacherId" property="teacherId"/>
        <result column="teacher2Id" property="teacher2Id"/>
        <result column="needStudent" property="needStudent"/>
        <result column="sourceId" property="source"/>
        <result column="typeId" property="typeId"/>
        <result column="propertyId" property="propertyId"/>
        <result column="compare" property="compare"/>
        <result column="content" property="content"/>
        <result column="result" property="result"/>
        <result column="reference" property="reference"/>
        <result column="planLinkKey" property="planLinkKey"/>
        <result column="teacherVerifyId" property="teacherVerifyId"/>
        <result column="adminVerifyId" property="adminVerifyId"/>
        <result column="createDt" property="createDt"/>
        <result column="createBy" property="createBy"/>
        <result column="validityBatch" property="validityBatch"/>
        <result column="selectStatus" property="selectStatus"/>
    </resultMap>
    <sql id="Base_From">
        Topic
    </sql>
    <insert id="insertWithAll" parameterType="cn.aethli.gttss.domain.Topic">
        INSERT INTO
        <include refid="Base_From"/>
        (`id`, `name`, `teacherId`, `selectStatus`, `needStudent`, `sourceId`, `typeId`, `teacher2Id`, `propertyId`,
        `compare`, `content`, `result`, `reference`, `planLinkKey`, `teacherVerifyId`, `adminVerifyId`, `createDt`,
        `createBy`, `validityBatch`)
        VALUES
        (#{id},#{name},#{teacherId},#{selectStatus},#{needStudent},#{sourceId},#{typeId},#{teacher2Id},#{propertyId},
        #{compare},#{content},#{result},#{reference},#{planLinkKey},#{teacherVerifyId},#{adminVerifyId},#{createDt},#{createBy},
        #{validityBatch});
    </insert>
</mapper>