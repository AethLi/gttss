<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aethli.gttss.dao.TopicMapper">
    <resultMap id="BaseResultMap" type="cn.aethli.gttss.domain.Topic">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="teacherId" property="teacherId"/>
        <result column="teacher2Id" property="teacher2Id"/>
        <result column="needStudent" property="needStudent"/>
        <result column="source" property="source"/>
        <result column="properties" property="properties"/>
        <result column="compare" property="compare"/>
        <result column="content" property="content"/>
        <result column="result" property="result"/>
        <result column="reference" property="reference"/>
        <result column="planLinkKey" property="planLinkKey"/>
        <result column="teacherVerifyId" property="teacherVerifyId"/>
        <result column="adminVerifyId" property="adminVerifyId"/>
    </resultMap>
    <sql id="Base_From">
        SysUser
    </sql>
    <insert id="insertWithAll" parameterType="cn.aethli.gttss.domain.Topic">
        INSERT INTO
        <include refid="Base_From"/>
        (`userId`, `account`, `permission`, `password`, `status`)
        VALUES (#{userId}, #{account}, #{permission}, #{password},#{status});
    </insert>
</mapper>