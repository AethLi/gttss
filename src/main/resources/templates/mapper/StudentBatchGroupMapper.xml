<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aethli.gttss.dao.StudentBatchGroupMapper">
    <resultMap id="BaseResultMap" type="cn.aethli.gttss.domain.StudentBatchGroup">
        <result column="studentId" property="studentId"/>
        <result column="batchId" property="batchId"/>
    </resultMap>
    <sql id="Base_From">
        StudentBatchGroup
    </sql>
    <insert id="insertWithAllByList" parameterType="java.util.List">
        insert into StudentBatchGroupMapper (`studentId`,`batchId`)values
        <foreach item="item" collection="list" separator=",">
            (#{item.studentId}, #{item.batchId})
        </foreach>
    </insert>
    <insert id="insertWithAll" parameterType="cn.aethli.gttss.domain.StudentBatchGroup">
        insert into
        <include refid="Base_From"/>
        (`studentId`,`batchId`)values (#{studentId},#{batchId});
    </insert>
</mapper>