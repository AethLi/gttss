<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aethli.gttss.dao.StudentBatchGroupMapper">
    <resultMap id="BaseResultMap" type="cn.aethli.gttss.domain.StudentBatchGroup">
        <result column="studentId" property="studentId"/>
        <result column="batchCode" property="batchCode"/>
    </resultMap>
    <insert id="insertWithAllByList" parameterType="java.util.List">
        insert into StudentBatchGroupMapper (`studentId`,`batchCode`)values
        <foreach item="item" collection="list" separator=",">
            (#{item.studentId}, #{item.batchCode})
        </foreach>
    </insert>
    <insert id="insertWithAll" parameterType="cn.aethli.gttss.domain.StudentBatchGroup">
        insert into StudentBatchGroupMapper (`studentId`,`batchCode`)values (#{studentId},#{batchCode});
    </insert>
</mapper>